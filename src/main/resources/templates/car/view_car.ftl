        });

        $(".select .dd ul li").click(function() {
            var text = $(this).html();
            $(this).closest('.select').find('.dt span').html(text);
            $(this).closest('.select').find('.dd ul').hide();
            $(this).closest('.select').find('.select_value').val($(this).html());
        });


        $(document).bind('click', function(e) {
